{"components/products/modals/modal-quote-form":"<div class=\"ogClosed\">\r\n\r\n\r\n  <div id=\"modal-quote-form\" class=\"center-modal\" data-reveal>\r\n  \r\n    <div class=\"modal-header\">\r\n        <h2 class=\"modal-header-title\">Submit A Quote</h2>\r\n        <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n            <span aria-hidden=\"true\">&#215;</span>\r\n        </a>\r\n    </div>\r\n    <div class=\"modal-body\">\r\n        <div class=\"writeReview-productDetails\">\r\n            <img src=\"{{getImage product.main_image 'product_size' (cdn theme_settings.default_image_product)}}\">\r\n            <h6 class=\"product-brand\">{{ product.brand.name }}</h6>\r\n            <h5 class=\"product-title\">{{ product.title }}</h5>\r\n        </div>\r\n        <form id=\"contactForm\">\r\n          <p>\r\n            <label>Name</label>\r\n            <input class=\"name1\" type=\"text\" name=\"name\" id=\"name\" required>\r\n          </p>\r\n          <p>\r\n            <label>Company</label>\r\n            <input class=\"company1\" type=\"text\" name=\"company\" id=\"company\">\r\n          </p>\r\n          <p>\r\n            <label>Email Address</label>\r\n            <input class=\"email-address-1\" type=\"email\" name=\"email\" id=\"email\" required>\r\n          </p>\r\n          <p>\r\n            <label>Phone Number</label>\r\n            <input class=\"phone-number-1\" type=\"text\" name=\"phone\" id=\"phone\">\r\n          </p>\r\n          <p class=\"full\">\r\n            <label>Message</label>\r\n            <textarea class=\"message-1\" name=\"message\" rows=\"5\" id=\"message\"></textarea>\r\n          </p>\r\n          <p class=\"full\">\r\n            <button id=\"firebase-submit\" type=\"submit\">Submit</button>\r\n          </p>\r\n        </form>\r\n  \r\n        <script src=\"https://www.gstatic.com/firebasejs/4.3.0/firebase.js\"></script>\r\n        <script src=\"product.js\"></script>\r\n     </div>\r\n  </div>\r\n  </div>\r\n  \r\n  \r\n  \n"}
